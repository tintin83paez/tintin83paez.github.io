  "components": {
    "schemas": {
      "DeviceForLeavingReviewDTO": {
        "required": [
          "brand",
          "model",
          "name",
          "year"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "brand": {
            "minLength": 1,
            "type": "string"
          },
          "color": {
            "type": "string",
            "nullable": true
          },
          "model": {
            "minLength": 1,
            "type": "string"
          },
          "year": {
            "maximum": 2100,
            "minimum": 1900,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "DeviceForPurchaseDTO": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "model": {
            "type": "string",
            "nullable": true
          },
          "color": {
            "type": "string",
            "nullable": true
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "priceForPurchase": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "DeviceForRentalDTO": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "model": {
            "type": "string",
            "nullable": true
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "color": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "rentalPrice": {
            "type": "number",
            "format": "double"
          },
          "quality": {
            "$ref": "#/components/schemas/QualityType"
          }
        },
        "additionalProperties": false
      },
      "PaymentMethodTypes": {
        "enum": [
          "CreditCard",
          "PayPal",
          "Cash"
        ],
        "type": "string"
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "PurchaseDetailDTO": {
        "required": [
          "deliveryAddress",
          "nameSurname",
          "paymentMethod",
          "userName"
        ],
        "type": "object",
        "properties": {
          "deliveryAddress": {
            "minLength": 1,
            "type": "string",
            "format": "multiline"
          },
          "purchaseItems": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PurchaseItemDTO"
            },
            "nullable": true
          },
